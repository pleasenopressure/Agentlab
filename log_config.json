{
    "version": 1,
    "disable_existing_loggers": true,
    "formatters": {
        "default": {
            "format": "%(asctime)s | %(levelname)s | %(name)s | %(message)s"
        }
    },
    "handlers": {
        "file": {
            "class": "logging.FileHandler",
            "formatter": "default",
            "filename": "logs/agentlab.log",
            "mode": "w",
            "encoding": "utf-8"
        },
        "console": {
            "class": "logging.StreamHandler",
            "formatter": "default",
            "stream": "ext://sys.stdout"
        }
    },
    "root": {
        "level": "INFO",
        "handlers": [
            "file"
        ]
    },
    "loggers": {
        "uvicorn": {
            "level": "INFO",
            "handlers": [
                "file"
            ],
            "propagate": false
        },
        "uvicorn.error": {
            "level": "INFO",
            "handlers": [
                "file"
            ],
            "propagate": false
        },
        "uvicorn.access": {
            "level": "INFO",
            "handlers": [
                "file"
            ],
            "propagate": false
        }
    }
}